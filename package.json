{
  "name": "rddl-for-vscode",
  "displayName": "RDDL for VSCode",
  "description": "Language support for RDDL",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "RDDL",
    "automated planning",
    "probabilistic planning",
    "AI",
    "artificial intelligence"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danielbdias/rddl-for-vscode"
  },
  "scripts": {
    "vscode-package": "vsce package"
  },
  "contributes": {
    "languages": [{
      "id": "rddl",
      "aliases": ["RDDL", "rddl"],
      "extensions": [".rddl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rddl",
      "scopeName": "domain.rddl",
      "path": "./syntaxes/rddl.tmLanguage.json"
    }]
  }
}
